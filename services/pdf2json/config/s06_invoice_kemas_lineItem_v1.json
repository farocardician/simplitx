{
  "fields": {
    "description": {
      "header_synonyms": [
        "\\bDESCRIPTION\\b",
        "DESCRIPTION OF GOODS",
        "DESCRIPTION OF SERVICES",
        "\\bNO\\b"
      ],
      "position_hint": {
        "x0": 0.08,
        "x1": 0.6
      },
      "parsers": [
        "strip_nonprint"
      ],
      "required": true,
      "merge": true,
      "merge_wrapped": true,
      "glue_hyphenated_words": true,
      "postprocess": [
        {
          "pattern": "^\\s*\\d+\\s+",
          "replace": ""
        },
        {
          "pattern": "-\\s*(?=[A-Za-z])",
          "replace": "- "
        },
        {
          "pattern": "\\b\\d+\\s+(Komersial,)",
          "replace": "\\1"
        },
        {
          "pattern": "^(Pekerjaan) Komersial, (Pembuatan Materi Promosi) (.*)$",
          "replace": "\\1 \\2 Komersial, \\3"
        },
        {
          "pattern": "\\s-\\s*(?=\\d)",
          "replace": " - "
        }
      ]
    },
    "qty": {
      "header_synonyms": [
        "\\bQTY\\b",
        "\\bQUANTITY\\b"
      ],
      "position_hint": {
        "x0": 0.37,
        "x1": 0.56
      },
      "parsers": [
        "parse_number",
        "split_qty_uom"
      ],
      "required": true,
      "merge": false
    },
    "unit_price": {
      "header_synonyms": [
        "UNIT PRICE",
        "UNIT PRICE (IDR)",
        "UNIT COST",
        "PRICE",
        "RATE",
        "\\(Rp\\.?\\)"
      ],
      "position_hint": {
        "x0": 0.56,
        "x1": 0.90
      },
      "parsers": [
        "parse_money"
      ],
      "required": false,
      "merge": false
    },
    "amount": {
      "header_synonyms": [
        "AMOUNT",
        "AMOUNT (IDR)",
        "TOTAL",
        "LINE TOTAL"
      ],
      "position_hint": {
        "x0": 0.73,
        "x1": 0.90
      },
      "parsers": [
        "parse_money"
      ],
      "required": true,
      "merge": false
    }
  },
  "currency_hints": [
    "IDR",
    "Rp",
    "RP"
  ],
  "number_format": {
    "decimal": ",",
    "thousands": ".",
    "allow_parens": true
  },
  "currency_decimals": 0,
  "tolerances": {
    "amount_from_qty_price": {
      "abs": 0,
      "rel": 0
    }
  },
  "defaults": {
    "discount_percent": 0,
    "discount_amount": 0,
    "qty": 1
  },
  "derivation": {
    "fill_amount_from_qty_price": true,
    "fill_unit_price_from_amount_qty": true
  },
  "row_filters": [
    "^\\s*(TOTAL|SUB\\s*TOTAL|GRAND\\s*TOTAL|VAT|PPN|TAX BASE)\\s*$",
    "^\\s*NO\\s*$",
    "^\\s*DESCRIPTION\\s*$",
    "^\\s*QTY\\s*$",
    "^\\s*UOM\\s*$",
    "^\\s*UNIT\\s*PRICE\\s*$",
    "^\\s*TOTAL\\s*$",
    "^\\s*#\\s*$"
  ]
}
