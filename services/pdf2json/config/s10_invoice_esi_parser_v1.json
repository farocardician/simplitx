{
  "metadata": {
    "template": "ESI Invoice (IDR, VAT 12%)",
    "money_rounding": 0
  },
  "defaults": {
    "currency": "IDR",
    "tax_label": "VAT 12%"
  },
  "header": {
    "pages": [1],
    "search_row_limit": 30,
    "fields": {
      "invoice_number": { "match": "contains" },
      "invoice_date": { "match": "contains" }
    }
  },
  "items": {
    "min_columns": 4,
    "header_rows_can_hold_items": false,
    "key_field": "no",
    "fields": [
      { "name": "no", "column": 0, "type": "int" },
      { "name": "description", "column": 2, "transform": "string" },
      { "name": "qty", "column": 3, "transform": "int" },
      { "name": "unit_price", "column": 6, "transform": "money" },
      { "name": "amount", "column": 8, "transform": "money" }
    ]
  },
  "totals": {
    "tax_label": "VAT 12%",
    "fields": {
      "subtotal": [
        "printed.subtotal",
        "checks.subtotal.computed"
      ],
      "tax_base": [
        "printed.tax_base",
        "computed.tax_base"
      ],
      "tax_amount": [
        "printed.tax_amount",
        "computed.tax_amount"
      ],
      "grand_total": [
        "printed.grand_total",
        "computed.grand_total"
      ]
    }
  },
  "manifest": {
    "schema_template": "ESI Invoice (IDR, VAT 12%)"
  }
}